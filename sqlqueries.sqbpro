/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="playlists" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_" freeze_columns="0"><sort/><column_widths><column index="1" value="61"/><column index="2" value="210"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="customers from USA">SELECT *
FROM customers
WHERE Country=&quot;USA&quot;




</sql><sql name="total sales per customer">SELECT c.CustomerId,
       c.FirstName || ' ' || c.LastName AS FullName,
       SUM(i.Total) AS TotalSpent
FROM customers c
JOIN invoices i ON c.CustomerId = i.CustomerId
GROUP BY c.CustomerId
ORDER BY TotalSpent DESC;</sql><sql name="total sales per country">SELECT BillingCountry,
       SUM(Total) AS Revenue
FROM invoices
GROUP BY BillingCountry
ORDER BY Revenue DESC;
</sql><sql name="INNER JOIN – Show invoice details with track info">SELECT i.InvoiceId,
       c.FirstName || ' ' || c.LastName AS CustomerName,
       t.Name AS Track,
       il.UnitPrice,
       il.Quantity
FROM invoice_items il
JOIN invoices i ON il.InvoiceId = i.InvoiceId
JOIN customers c ON i.CustomerId = c.CustomerId
JOIN tracks t ON il.TrackId = t.TrackId;
</sql><sql name="LEFT JOIN – Find customers who never purchased anything">SELECT c.CustomerId,
       c.FirstName,
       c.LastName
FROM customers c
LEFT JOIN invoices i ON c.CustomerId = i.CustomerId
WHERE i.InvoiceId IS NULL;
</sql><sql name="Average revenue per invoice (ARPU equivalent)">SELECT AVG(Total) AS AverageRevenue
FROM invoices;
</sql><sql name="Subquery – Get customers who spent more than the average spent">SELECT CustomerId,
       SUM(Total) AS TotalSpent
FROM invoices
GROUP BY CustomerId
HAVING TotalSpent &gt; (
        SELECT AVG(Total)
        FROM invoices
);
</sql><sql name="Handling NULL values">SELECT CustomerId,
       FirstName,
       LastName,
       COALESCE(State, 'Unknown') AS State
FROM customers;
</sql><sql name="High value customers (&gt; $50)">CREATE VIEW HighValueCustomers AS
SELECT c.CustomerId,
       c.FirstName || ' ' || c.LastName AS FullName,
       SUM(i.Total) AS TotalSpent
FROM customers c
JOIN invoices i ON c.CustomerId = i.CustomerId
GROUP BY c.CustomerId
HAVING TotalSpent &gt; 50;
</sql><sql name="optimization" filename="C:/Users/Deepika/Downloads/sqlqueries.sql">-- Reference to file &quot;C:/Users/Deepika/Downloads/sqlqueries.sql&quot; (not supported by this version) --</sql><current_tab id="2"/></tab_sql></sqlb_project>
